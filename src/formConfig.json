[
    {
      "type": "text",
      "name": "username",
      "label": "Username",
      "validation": {
        "required": true,
        "regex": "^[a-zA-Z0-9]{3,}$"
      }
    },
    {
      "type": "password",
      "name": "password",
      "label": "Password",
      "validation": {
        "required": true,
        "regex": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$"
      }
    },
    {
      "type": "select",
      "name": "gender",
      "label": "Gender",
      "options": ["Select", "Male", "Female"],
      "validation": {
        "required": true
      }
    },
    {
      "type": "radio",
      "name": "subscription",
      "label": "Subscription",
      "options": ["Basic", "Premium"],
      "validation": {
        "required": true
      }
    },
    {
      "type": "checkbox",
      "name": "agree",
      "label": "I agree to the terms and conditions",
      "validation": {
        "required": true
      }
    },
    {
      "type": "file",
      "name": "resume",
      "label": "Upload Resume",
      "validation": {
        "required": false
      }
    }
  ]
  